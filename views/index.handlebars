<div id = "toBeDevoured">
    <ul>
        {{#each burgers}}
        {{#unless devoured}}
        <li class = "burger"><button class ="devour" data-id="{{id}}">Devour!</button> {{burger_name}} </li>
        
        {{/unless}}
        {{/each}}
    </ul>
</div>

<div id = "devoured">
    <ul>
        {{#each burgers}}
        {{#if devoured}}
        <li class = "burger"> {{burger_name}}</li>
        {{/if}}
        {{/each}}
    </ul>
</div>

<form id="create-form">

  <div class="form-group">
    <label for="newBurger">New burger name:</label>
    <input type="text" id="newBurger" name="name">
  </div>
 <button id = "addBurger" type="submit">Add new burger</button>
</form>



<button onclick="myFunction()">Add new button</button>
